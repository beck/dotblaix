[color]
  ui = auto

[alias]
  co = checkout
  st = status
  ci = commit -v
  gr = log --graph --full-history --all --color --pretty=format:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s"
  fu = !git fetch origin && git st && git log master..origin/master

[user]
  name = Justin Blake
  email = justin@hentzia.com

[github]
  user = blaix
  token = *secret*

[branch]
  autosetuprebase = always

[core]
  mergeoptions = "--no-commit"
	excludesfile = /Users/jblake/.gitignore
